<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_gravity="bottom|left"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:showIn="@layout/activity_main_feed"
    >

    <android.support.design.widget.FloatingActionButton
        android:layout_width="54dp"
        android:layout_height="54dp"
        android:layout_gravity="bottom|start"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="16dp"
        android:src="@drawable/menu_open"
        android:elevation="0dp"
        android:id="@+id/mainFAB"
        app:pressedTranslationZ="12dp"
        />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|start"
        android:layout_marginLeft="15dp"
        android:layout_marginBottom="80dp"
        android:src="@drawable/upload"
        android:elevation="0dp"
        android:id="@+id/cameraFAB"
        app:pressedTranslationZ="12dp"
        android:stateListAnimator="@null"
        android:visibility="gone"
        />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|start"
        android:layout_marginLeft="15dp"
        android:layout_marginBottom="130dp"
        android:src="@drawable/competition"
        android:elevation="0dp"
        android:stateListAnimator="@null"
        android:id="@+id/competitionFAB"
        app:pressedTranslationZ="12dp"
        android:visibility="gone"
        />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|start"
        android:layout_marginLeft="15dp"
        android:layout_marginBottom="180dp"
        android:src="@drawable/privacy_mutual_follower_photo"
        android:elevation="0dp"
        android:id="@+id/profileFAB"
        android:stateListAnimator="@null"
        app:pressedTranslationZ="12dp"
        android:visibility="gone"
        />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|start"
        android:layout_marginLeft="90dp"
        android:layout_marginBottom="20dp"
        android:src="@drawable/notifications_icon"
        android:elevation="0dp"
        android:stateListAnimator="@null"
        android:id="@+id/notificationFAB"
        app:pressedTranslationZ="12dp"
        android:visibility="gone"
        />




</android.support.design.widget.CoordinatorLayout>